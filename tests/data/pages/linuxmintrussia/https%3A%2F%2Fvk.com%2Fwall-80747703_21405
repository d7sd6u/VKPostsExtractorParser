  <div class="ui_actions_menu_icons" tabindex="0" aria-label="Actions" role="button" onclick="uiActionsMenu.keyToggle(this, event);"> <span class="blind_label">Actions</span> </div>
  <div class="ui_actions_menu _ui_menu " ><a class="ui_actions_menu_item" onclick="wall.markAsSpam(this, '-80747703_21405', '502573db877d3ffd38');" tabindex="0" role="link">Report</a></div>
